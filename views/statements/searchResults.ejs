<!DOCTYPE html>
<html>

<head>
    <title>Search Results</title>
</head>

<body>
    <h1>Search Results</h1>
    <% if (results.length> 0) { %>
        <ul>
            <% results.forEach(function(result) { %>
                <li>
                    <h2>
                        <%= result.title %>
                    </h2>
                    <p>
                        <%= result.actors %>
                    </p>
                    <p>
                        <%= result.NCPGoodOffices %>
                    </p>
                    <p>
                        <%= result.summary %>
                    </p>
                    <!-- Display other fields as needed -->
                </li>
                <% }); %>
        </ul>
        <% } else { %>
            <p>No results found.</p>
            <% } %>
</body>

</html>